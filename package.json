{
  "name": "dom-to-jsx",
  "version": "0.0.0",
  "description": "Get jsx from DOM",
  "main": "dist/index.js",
  "scripts": {
    "test": "ava --verbose && semistandard | snazzy",
    "build": "rm -rf dist && mkdir -p dist && babel lib --out-dir dist",
    "watch": "rm -rf dist && mkdir -p dist && babel -w lib --out-dir dist",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/micnews/dom-to-jsx.git"
  },
  "keywords": [
    "jsx",
    "dom"
  ],
  "author": "mic.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/micnews/dom-to-jsx/issues"
  },
  "homepage": "https://github.com/micnews/dom-to-jsx#readme",
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-preset-es2015": "^6.6.0",
    "semistandard": "^7.0.5",
    "snazzy": "^3.0.1"
  }
}